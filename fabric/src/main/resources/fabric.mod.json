{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${mod_version}",
  "name": "${mod_name}",
  "description": "${mod_description}",
  "authors": ["${mod_authors}"],
  "contact": {
    "sources": "${display_url}",
    "issues": "${issue_tracker_url}",
    "homepage": "${homepage_url}"
  },
  "license": "${license}",
  "icon": "${logo_file}",
  "environment": "*",
  "entrypoints": {
    "main": [
      "whocraft.tardis_refined.fabric.TardisRefinedFabric"
    ],
    "cardinal-components": [
      "whocraft.tardis_refined.common.capability.fabric.TRComponents"
    ],
    "client": [
      "whocraft.tardis_refined.fabric.TardisRefinedFabricClient"
    ]
  },
  "mixins": [
    "${mod_id}.mixins.json",
    "${mod_id}-common.mixins.json"
  ],
  "depends": {
    "fabricloader": "${fabric_loader_version_range}",
    "minecraft": "${fabric_minecraft_version_range}",
    "fabric-api": "${fabric_version_range}"
  },
  "custom": {
    "cardinal-components": [
      "${mod_id}:tardis_data"
    ]
  }
}
